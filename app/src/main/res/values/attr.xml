<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TopBar">
        <attr name="left_button_text_back" format="reference|color"/>

        <attr name="title_color" format="color"/>
        <attr name="title_name" format="string"/>
        <attr name="title_size" format="dimension"/>
        <attr name="title_background" format="reference|color"/>

        <attr name="right_button_text_color" format="color"/>
        <attr name="right_button_title" format="string"/>
        <attr name="right_button_size" format="dimension"/>
        <attr name="right_button_text_back" format="reference|color"/>


    </declare-styleable>
</resources>